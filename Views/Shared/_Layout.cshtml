<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <title>DDU ERP</title>

   
    <!-- Plugins Core Css -->
  
    <link href="~/assets/css/form.min.css" rel="stylesheet">
 
    <!-- Plugins Core Css -->
    <link href="~/assets/css/common.min.css" rel="stylesheet">
    <link href="~/assets/css/form.min.css" rel="stylesheet">

    <!-- Custom Css -->
    <link href="~/assets/css/style.css" rel="stylesheet" />
    <!-- You can choose a theme from css/styles instead of get all themes -->
    <link href="~/assets/css/styles/all-themes.css" rel="stylesheet" />
    <style>
        .sectionHeader {
             display:flex; 
             justify-content:space-between;
             align-items: center;
        }
        .active {
            color: #3a71c7 !important;
        }
        .header > ul > li {
            color: gray !important;
            font-weight: 300;
        }
    </style>
</head>

<body>
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30">
                <img class="loading-img-spin" src="~/assets/images/loading.png" alt="admin">
            </div>
            <p>Please wait...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
    <!-- Top Bar -->
    @Html.Partial("_TopBar.cshtml")
    <!-- #Top Bar -->
    <div>
        <!-- Left Sidebar -->
        @Html.Partial("_Menu.cshtml")
        <!-- #END# Left Sidebar -->
    </div>
    <section class="content">
        @RenderSection("Content")
        
    </section>

    <div class="modal" tabindex="-1" role="dialog" id="form-modal" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"></span>
                    </button>
                </div>
                <div class="modal-body">

                </div>
            </div>
        </div>
    </div>


     <!-- Plugins Js -->
  
    <script src="~/assets/js/bundles/flatpickr.min.js"></script>
    <script src="~/assets/js/bundles/autosize.js"></script>
    <script src="~/assets/js/common.min.js"></script>
    <!-- Custom Js -->
    <script src="~/assets/js/admin.js"></script>
    <script src="~/assets/js/bundles/echart/echarts.js"></script>
    <script src="~/assets/js/bundles/apexcharts/apexcharts.min.js"></script>
    <script src="~/assets/js/pages/index.js"></script>
    <script src="~/assets/js/pages/todo/todo.js"></script>
    <script src="~/assets/js/bundles/dropzone.js"></script>
    <script src="~/assets/js/bundles/flatpickr.min.js"></script>
    <script src="~/assets/js/pages/forms/flatpicker-data.js"></script>
    <script src="~/assets/js/pages/forms/dropzone-data.js"></script>
    <script src="~/assets/js/pages/forms/selectitem-data.js"></script>
    <script src="~/assets/js/pages/forms/basic-form-elements.js"></script>

       @* <script src="~/lib/jquery/dist/jquery.js"></script>*@
       @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
            integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU="
            crossorigin="anonymous"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    

</body>

</html>